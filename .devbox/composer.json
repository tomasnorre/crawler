{
    "name": "tomasnorre/crawler-devbox",
    "description": "Devbox for TYPO3 Crawler",
    "license": "GPL-3.0",
    "type": "project",
    "authors": [
        {
            "name": "Tomas Norre Mikkelsen",
            "email": "tomasnorre@gmail.com",
            "role": "Developer"
        }
    ],
    "homepage": "https://github.com/tomasnorre/crawler",
    "support": {
        "issues": "https://github.com/tomasnorre/crawler/issues"
    },
    "require": {
        "georgringer/news": "^11.4",
        "tnm/crawler-devbox-sitepackage": "^0.0.2",
        "tomasnorre/crawler": "*@dev",
        "typo3/cms-belog": "^12.4 || dev-main" ,
        "typo3/cms-beuser": "^12.4 || dev-main",
        "typo3/cms-felogin": "^12.4 || dev-main",
        "typo3/cms-indexed-search": "^12.4 || dev-main",
        "typo3/cms-info": "^12.4 || dev-main",
        "typo3/cms-tstemplate": "^12.4 || dev-main",
        "typo3/minimal": "^12.4 || dev-main"
    },
    "require-dev": {
        "roave/security-advisories": "dev-latest"
    },
    "conflict": {
        "tomasnorre/crawler": "<12.0.0"
    },
    "repositories": [
        {
            "type": "path",
            "url": "./packages"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "allow-plugins": {
            "typo3/cms-composer-installers": true,
            "typo3/class-alias-loader": true
        },
        "bin-dir": "bin",
        "vendor-dir": "vendor"
    },
    "extra": {
        "typo3/cms": {
            "cms-package-dir": "{$vendor-dir}/typo3/cms",
            "web-dir": "public"
        }
    }
}
