{
    "name": "tomasnorre/crawler-devbox",
    "type": "project",
    "description": "Devbox for TYPO3 Crawler",
    "homepage": "https://github.com/tomasnorre/crawler",
    "support": {
        "issues": "https://github.com/tomasnorre/crawler/issues"
    },
    "license": "GPL-3.0",
    "authors": [
        {
            "name": "Tomas Norre Mikkelsen",
            "email": "tomasnorre@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "typo3/minimal": "^11.5 || 12.1.x-dev",
        "typo3/cms-tstemplate": "^11.5 || 12.1.x-dev",
        "typo3/cms-info": "^11.5 || 12.1.x-dev",
        "typo3/cms-belog": "^11.5 || 12.1.x-dev",
        "georgringer/news": "^9.0",
        "helhum/typo3-console": "*",
        "tomasnorre/crawler": "dev-main",
        "tnm/crawler-devbox-sitepackage": "^0.0.2",
        "typo3/cms-beuser": "^11.5 || 12.1.x-dev",
        "typo3/cms-indexed-search": "^11.5 || 12.1.x-dev",
        "typo3/cms-felogin": "^11.5 || 12.1.x-dev"
    },
    "repositories": [
        {
            "type": "path",
            "url": "./packages"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "vendor-dir": "vendor",
        "bin-dir": "bin",
        "allow-plugins": {
            "typo3/cms-composer-installers": true,
            "typo3/class-alias-loader": true
        }
    },
    "extra": {
        "typo3/cms": {
            "cms-package-dir": "{$vendor-dir}/typo3/cms",
            "web-dir": "public"
        }
    }
}
